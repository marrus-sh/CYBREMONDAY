{% assign result = nil %}{% if include.for %}{% for file in site.pages %}{% assign pstart = file.path | slice: 0 %}{% if pstart != "/" %}{% assign path = file.path | prepend: "/" %}{% else %}{% assign path = file.path %}{% endif %}{% if path == include.for %}{% assign result = file.url %}{% break %}{% endif %}{% endfor %}{% unless result %}{% for file in site.documents %}{% assign pstart = file.path | slice: 0 %}{% if pstart != "/" %}{% assign path = file.path | prepend: "/" %}{% else %}{% assign path = file.path %}{% endif %}{% if path == include.for %}{% assign result = file.url %}{% break %}{% endif %}{% endfor %}{% endunless %}{% unless result %}{% for file in site.static_files %}{% assign pstart = file.path | slice: 0 %}{% if pstart != "/" %}{% assign path = file.path | prepend: "/" %}{% else %}{% assign path = file.path %}{% endif %}{% if path == include.for %}{% assign result = file.url | default: file.path %}{% break %}{% endif %}{% endfor %}{% endunless %}{% endif %}{% if include.absolute %}{{ result | absolute_url }}{% else %}{{ result | relative_url }}{% endif %}
